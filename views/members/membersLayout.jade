extends ../layout

block styles
	link(rel='stylesheet' type='text/css' href='/css/members_layout.css')
	block membersStyles

block content
	div.navbar-container
		div.navbar
			div.navbar-inner
				ul.nav
					if level == 1
						li#li_Admin 
							a(href='/Admin') Admin
						li.divider-vertical
					li#li_Members
						a(href='/MembersOfMIPS') Members
					if level < 3
						li.divider-vertical
						li#li_AddMember
							a(href='/MembersOfMIPS/AddMember') Add New Member

	div#membersContainer
		block membersContent